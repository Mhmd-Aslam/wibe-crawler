[
  {
    "label": "Electron: Main",
    "adapter": "Node",
    "request": "launch",
    "cwd": "${ZED_WORKTREE_ROOT}",
    "runtimeExecutable": "${ZED_WORKTREE_ROOT}/node_modules/.bin/electron-vite",
    "runtimeArgs": ["--sourcemap", "--inspect"],
    "env": {
      "NODE_ENV": "development"
    }
  },
  {
    "label": "Electron: All",
    "adapter": "Node",
    "request": "launch",
    "cwd": "${ZED_WORKTREE_ROOT}",
    "runtimeExecutable": "${ZED_WORKTREE_ROOT}/node_modules/.bin/electron",
    "args": ["--inspect=5858", "--remote-debugging-port=9222", "."],
    "outputCapture": "std",
    "env": {
      "NODE_ENV": "development"
    }
  }
]
